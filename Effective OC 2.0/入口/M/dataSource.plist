		<array>
			<string>Item6VC</string>
			<string>Item7VC</string>
			<string>Item8VC</string>
			<string>Item9VC</string>
			<string>Item10VC</string>
			<string>Item11VC</string>
			<string>Item12VC</string>
			<string>Item13VC</string>
			<string>Item14VC</string>
		</array>
		<key>isShow</key>
		<true/>
	</dict>
</array>
